#Could probably be optimized a bit in terms of more inclusive ranges:
IPTABLES=&quot;iptables -A OUTPUT&quot;
COMMENT=&quot; -m comment --comment &quot;
ACTION=&quot;-j DROP&quot;
$IPTABLES -m iprange --src-range 6.0.0.0-6.255.255.255 $ACTION  $COMMENT &quot;Army\ Information\ Systems\ Center&quot;
$IPTABLES -m iprange --src-range 7.0.0.0-7.255.255.255 $ACTION  $COMMENT &quot;Defense\ Information\ Systems\ Agency\,\ VA&quot;
$IPTABLES -m iprange --src-range 11.0.0.0-11.255.255.255 $ACTION  $COMMENT &quot;DoD\ Intel\ Information\ Systems\,\ Defense\ Intelligence\ Agency\,\ Washington\ DC&quot;
$IPTABLES -m iprange --src-range 21.0.0.0-21.255.255.255 $ACTION  $COMMENT &quot;US\ Defense\ Information\ Systems\ Agency&quot;
